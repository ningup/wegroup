<!doctype html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<title>成员列表</title>
<meta name="renderer" content="webkit">
<meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
<meta name="Keywords" content="微群助手－群成员" />
<meta name="Description" content="微信公众平台上的公众号－－微群助手" />
<link rel="stylesheet" type="text/css" href="/stylesheets/index.css">
<link rel="stylesheet" type="text/css" href="/stylesheets/skin.css">
<link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.css">
<script language="javascript" type="text/javascript" src="/javascript/jquery.min.js"></script>
<script language="javascript" type="text/javascript" src="/javascript/bootstrap.js"></script>
</head>
<body>
	<div class="search_button">
		<div class="input-group form-control">
			<span class="glyphicon glyphicon-search" id="sizing-addon2"></span>
			<input type="text" id="search_box" class="" placeholder="查找" aria-describedby="sizing-addon2">
		</div>
	</div>
	<div class="chat_list">
			<div class="media media_content" style="margin-top:0"><a href="#" class="symbol_img">
				<div class="media-left media-middle"><i class="glyphicon glyphicon-plus" style="width: 64px;text-align: center;height: 64px;font-size: 2em;padding-top: 14px;"></i></div>
				<div class="media-body media-middle"><h5 class="media-heading" style="margin-top: 0;"><strong>邀请成员</strong></h5></div>
				<div class="media-right media-middle"><i class="glyphicon glyphicon-chevron-right" style="width: 64px;text-align: center;height: 64px;font-size: 2em;padding-top: 14px;"></i></div>
			</a></div>
				<div class="media-body media-middle" style="margin: 9px 15px;display: block;width: auto;"><h5 class="media-heading" style="margin-top: 0;"><strong>成员</strong></h5></div>

		<ul class="list-unstyled">
        <for start="1" end="5">
			<div class="media media_content" data-toggle="modal" data-target="#channelModal" data-whatever="@mdo" style="margin-top:0"><a href="#" class="symbol_img">
				<div class="media-left media-middle"><img class="media-object img-circle" data-src="holder.js/46x46" alt="46x46" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 64px; height: 64px;"></div>
				<div class="media-body media-middle">
					<h5 class="media-heading" style="margin-top: 0;"><strong>某某某</strong></h5>
				</div>              
			</a></div>
		</for>
        </ul>
        
		<div id="channelModal" class="modal fade bs-album-modal-sm text-center" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
			<div class="modal-dialog modal-sm">
				<div class="modal-content">
                	<div class="media-object" data-holder-rendered="true" style="width: 100%;height: 220px;border-bottom: solid 15px;background-size: 100% 100%;background: url(http://www.w3school.com.cn/i/bg_flower.gif) no-repeat center;padding-top: 150px;color:#FFF"><div class="bl-wi-color" style="height: 60px;top: 145px;"><h3></h3></div></div>
					<div class="modal-body">
                        <p class="text-center"></p>
						<button type="button" class="btn btn-success" style="width: 100%;">加为好友</button>
					</div>      
				</div>
			</div>
		</div>       
        
	</div>
<script language="javascript" type="text/javascript">
$(function(){
$('#channelModal').on('show.bs.modal', function (event) {
	//console.debug(this);
  var channel = $(event.relatedTarget);
  var modal = $(this)
  var recipient = channel.data('skin');//.text('New message to ' + recipient)
  channel_img = channel.find('img').text();
  channel_name = channel.find('h5').text();
  channel_contant = channel.find('h6').text();
  channel_small = channel.find('small').text();
  
  modal.find('img').text(channel_img)
  modal.find('h3').text(channel_name)
  modal.find('h5').text(channel_contant)
  modal.find('small').text(channel_small)
  modal.find('.modal-body input').val(recipient)
})
})
</script>
</body>
</html>