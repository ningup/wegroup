            }
        }
        function dowmPosition(){
            if (flag==1) {
                console.log("下拉加载");
                myScroll.refresh();
                flag = 0;
            }else if (flag==2) {
                console.log("上拉加载");
                for(var i=0;i<20;i++){
                    $(".container").append("<li>Pretty row 50</li>");
                }
                myScroll.refresh();
                flag = 0;
            }else{
                flag = 0;
            }
        }
