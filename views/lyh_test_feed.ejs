  <body>
    <div id="monitor">Y position: <strong id="position">0</strong></div>
    <div id="wrapper">
      <div class="container">

          <div class="feed-top">
            <div class="user-info">
              <div class="user-photo"><img src="<%= feed.get('userHeadImgUrl')%>"></div>
              <div class="user-name">
                <span class="user-nick"><%= feed.get('nicknameOfPUser')%></span>
              </div>
              <div class="arrow-right"></div>
            </div>
            <div class="feed-title"><%= feed.get('feedTitle')%></div>
            <div class="feed-info">
              <span class="time"><%= feed.getCreatedAt().getFullYear()%>-<%= feed.getCreatedAt().getMonth()+1%>-<%= feed.getCreatedAt().getDate()%> <%= feed.getCreatedAt().getHours()%>:<%= feed.getCreatedAt().getMinutes()%></span>
              <span class="band-name"><%= groupNickname%></span>
            </div>
          </div>

          <div class="feed-content">
            <div class="main-content">
                <div class="flag">
                  <div class="content"><%= feed.get('feedContent')%></div>
                  <% for(var f=0;f<feed.get('feedImgArray').length;f++){%>
                  <div class="pic"><img src="<%= feed.get('feedImgArray')[f]%>" data-index="<%= f%>"></div>
                  <% } %>
                </div>
            </div>
            <div class="actions">
                <% if(feed.get('postedBy')==username){ %>
                <span class="delete_feed">删除</span>
                <% } %>
              <span class="read-num">
                <i class="read-num-icon"></i>
                10000
              </span>
            </div>
          </div>

          <div class="comment-header">
                <div class="leftside-all-icon"></div>
                <span class="ping">全部评论</span>
                <span class="ping-num">一共<%= feed.get('commentCnt')%>条评论</span>
          </div>

          <div class="detail-comment-list">
            <div class="top-comment-wraper">
            <% if(havecomment==1){ %>
