                                var hours = date.getHours() - date1.getHours();
                                var minute = date.getMinutes() - date1.getMinutes();
                                var last_time = "";
                                if(year>0){
                                    last_time= year + "年前";
                                }else if(month>0){
                                    last_time= month + "月前";
                                }else if(day>0){
                                    last_time= day + "天前";
                                }else if(hours>0){
                                    last_time= hours + "小时前";
                                }else if(minute>0){
                                    last_time= minute + "分钟前";
                                }else{
