    .publish .pub-con .pub-wrap .pub-theme>input{border: 0;width: 100%;height: 45px;font-size: 15px;font-weight: 700;color: #000;outline: 0;-webkit-tap-highlight-color:rgba(255,255,255,0);padding: 0;margin: 0;border-radius: 0;-webkit-appearance:textfield;background-color: white;cursor: auto;font: 11px BlinkMacSystemFont;text-rendering:auto;letter-spacing: normal;word-spacing: normal;text-transform: none;text-indent: 0px;text-shadow:none;display: inline-block;text-align: start;-webkit-writing-mode: horizontal-tb;}
    .publish .pub-con .pub-wrap .pub-theme:after{border-color: #eee;bottom: -1px;display: block;position: absolute;left: 0;border-top: 1px solid #c8c7cc;width: 100%;content: " ";pointer-event:none;}
    .publish .pub-con .pub-wrap .pub-editor{padding: 10px 20px 15px;background: #fff;}
    .publish .pub-con .pub-wrap .pub-editor textarea{font-size: 14px;color: #000;margin: 0;display: -webkit-box;-webkit-box-flex:1;border: 0;width: 100%;height: auto;min-height: 72px;-webkit-appearance:none;outline: 0;-webkit-tap-highlight-color:rgba(255,255,255,0);border-radius: 0;background-color: white;-webkit-rtl-ordering:logical;-webkit-user-select:text;flex-direction:column;resize:auto;cursor: auto;white-space: pre-wrap;word-wrap:break-word;font: 11px BlinkMacSystemFont;text-rendering: auto;letter-spacing: normal;word-spacing: normal;text-transform: none;text-indent: 0px;text-shadow: none;text-align: start;-webkit-writing-mode: horizontal-tb;}
    .publish .pub-con .pub-wrap .pub-type{display: -webkit-box;-webkit-box-pack: end;-webkit-box-align: center;-webkit-box-sizing: content-box;padding: 10px 20px;height: 35px;font-size: 16px;background: #fff;list-style: none;margin: 0; }
    .publish .pub-con .pub-wrap .pub-type .selectPic{background-image: url(/images/t/test_touxiang.jpg);overflow: hidden;position: relative;margin-right: 14px;width: 25px;height: 25px;background: url(/images/t/test_touxiang.jpg) no-repeat 0 0;background-size: 25px 25px;line-height: 35px;}
    .publish .pub-con .pub-wrap .pub-type .selectFace{overflow: hidden;position: relative;width: 25px;height: 25px;background: url(/images/t/test_touxiang.jpg) no-repeat 0 0;background-size: 25px 25px;line-height: 35px;}
    .publish .pub-con .pub-wrap .pub-type .pub-flex{ -webkit-box-flex: 1; height: 35px;line-height: 35px;}
    .publish .pub-con .pub-wrap .pub-type .pub-btn{height: 35px;line-height: 35px;display: list-item;text-align: -webkit-match-parent;}
    .publish .pub-con .pub-wrap .pub-type .pub-btn button{border: 0;border-radius: 4px;
    width: 81px;
    height: 35px;
    font-size: 16px;
    text-align: center;
    color: #fff;float: right;
    background-color: #2791dc;outline: 0;
    -webkit-tap-highlight-color: rgba(255,255,255,0);padding: 0;
    margin: 0;font: 11px BlinkMacSystemFont;
    text-rendering: auto;
    letter-spacing: normal;
    word-spacing: normal;
    text-transform: none;
    text-indent: 0px;
    text-shadow: none;
    display: inline-block;}
    .publish .pub-con .pub-wrap .pub-type .pub-btn button.pub-cancel{margin-right: 10px;}
    .publish .pub-con .pub-wrap .pub-pics{display: block;    overflow: hidden;
    padding: 9px 18px 15px;list-style: none;margin: 0;    -webkit-margin-before: 1em;
    -webkit-margin-after: 1em;
    -webkit-margin-start: 0px;
    -webkit-margin-end: 0px;}
    .publish .pub-con .pub-wrap .pub-pics .up-entry{float: left;
    position: relative;
    margin: 6px 6px 0 0;
    width: 65px;
    height: 65px;background: transparent url(http://pub.idqqimg.com/qqun/xiaoqu/mobile/img/nopack/publish-sprite.png?9c568819) no-repeat -60px 0;
    background-size: 270px 65px;display: list-item;
    text-align: -webkit-match-parent;}
    .publish .pub-con .pub-wrap .pub-pics .up-entry .upfile{pointer-events: auto;position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
    cursor: pointer;outline: 0;
    -webkit-tap-highlight-color: rgba(255,255,255,0);padding: 0;
    margin: 0;border-radius: 0;-webkit-appearance: textfield;
    background-color: white;
    border: 2px inset;
    border-image-source: initial;
    border-image-slice: initial;
    border-image-width: initial;
    border-image-outset: initial;
    border-image-repeat: initial;
    -webkit-rtl-ordering: logical;
    -webkit-user-select: text;font: 11px BlinkMacSystemFont;
    text-rendering: auto;
    color: initial;
    letter-spacing: normal;
    word-spacing: normal;
    text-transform: none;
    text-indent: 0px;
    text-shadow: none;
    display: inline-block;
    text-align: start;}
    .publish .pub-con .pub-wrap .pub-pics .up-pic{
        float: left;
    position: relative;
    margin: 6px 6px 0 0;
    width: 65px;
    height: 65px;padding: 0;
    border: 0;
    font: inherit;
    font-size: 100%;
    vertical-align: baseline;display: list-item;
    text-align: -webkit-match-parent;
    }
    .publish .pub-con .pub-wrap .pub-pics .up-pic .clip{
        overflow: hidden;
    border-radius: 5px;
    width: 65px;
    height: 65px;margin: 0;
    padding: 0;
    border: 0;
    font: inherit;
    font-size: 100%;
    vertical-align: baseline;display: block;
    }
    .publish .pub-con .pub-wrap .pub-pics .up-pic .btn-del{
        display: block;position: absolute;
    top: 3px;
    right: 3px;
    border: 1px solid #fff;
    border-radius: 12px;
    width: 22px;
    height: 22px;
    background: rgba(0,0,0,.38);text-decoration: none;
    color: #0079ff;outline: 0;
    -webkit-tap-highlight-color: rgba(255,255,255,0);cursor: auto;
    }
    .publish .pub-con .pub-wrap .pub-pics .up-pic .btn-del:before{display: block;
    position: absolute;
    top: 11px;
    left: 4px;
    z-index: 2;
    width: 14px;
    height: 1px;
    background: #fff;
    -webkit-transform: rotate(45deg);
    content: "";}
