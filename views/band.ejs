    <style type="text/css">
    body {overflow: hidden;width: 100%;background-color: #efeff4;} /*background-color: #FBFBFB;*/
    #wrapper {position: absolute;overflow: hidden;left: 0;top: 0;bottom: 0;width: 100%;z-index: 1;}
    .container {position: relative;padding: 0;}
    #monitor {
    position: absolute;
    left: 51%;
    z-index: 2;
    }
    .band_top_info{
        position: relative;
        background-color: #fff;
        border-bottom: 1px solid #e1e3e6;
        padding: 12px 0 13px 9px;
        margin-bottom: 10px;
    }
    .band_top_info .top_portrait{
        float: left;
        margin-right: 13px;
        border-radius: 3px;
        width: 90px;
        height: 90px;
        background-size: 90px 90px;
        box-shadow: 0 0 1px 0 rgba(0,0,0,.3) inset;
        position: relative;
        background-repeat: no-repeat;
    }
    .top_title{
        color: #333;
        font-size: 18px;
        line-height: 18px;
        margin: 2px 0 0;
    }
    .top_title_txt{vertical-align: middle;}
    .top_paragraph{margin: 9px 0 0;line-height: 14px;font-size: 14px;overflow: hidden;word-break:break-all;}
    .top_paragraph_label:first-child{margin-left: 0;}
    .top_paragraph_label{color: #7a7c80;margin: 0 4px 0 12px;}
    .top_paragraph_txt{color: #f25824;}
    .top_sign_like{margin: 13px 1px 0 0;display: -webkit-box;position: relative;}
    .sign_like_time{font-size: 14px;line-height: 34px;height: 34px;display: inline-block;}
    /*.sign_like_button{width: 68px;height: 34px;position: absolute;right: 9px;top: 0;background-color: #ebedf0;color: #b2b6ba;border: 0;}*/
    .yes{width: 68px;height: 34px;position: absolute;right: 9px;top: 0;background-color: #ebedf0;color: #b2b6ba;border: 0;}
    .no{width: 68px;height: 34px;position: absolute;right: 9px;top: 0;background-color: #00a5e0;color: #ffffff;border: 0;}
    .feed{overflow: hidden;position: relative;margin-bottom: 10px;background-color: #fff;padding-top: 18px;}
    .feed .detail{padding: 0 15px 15px 15px;display: block;overflow: hidden;position: relative;margin: 0;}
    .feed .detail .feed_content{overflow: hidden;margin-bottom: 10px;max-height: 73px;}
    .feed .detail .feed_content .feed_title{display: -webkit-box;overflow: hidden;-webkit-box-orient:vertical;text-overflow:ellipsis;word-break:break-all;-webkit-line-clamp:2;font-size: 21px;line-height: 25px;color: #000;padding: 0;margin: 0;}
    .feed .detail .feed_content .feed_txt{display: -webkit-box;overflow: hidden;-webkit-box-orient:vertical;text-overflow:ellipsis;word-break:break-all;-webkit-line-clamp:2;padding-top: 5px;padding-left: 1px;padding-bottom: 0; margin: 0; font-size: 14px;line-height: 18px;color: #777;}
    .feed .detail .feed_img .feed_img_item{width: 32%;float: left;margin-right: 3px;background: none;overflow: hidden;position: relative;}
    .feed .detail .feed_img .feed_img_item>img{width: 100%;height: auto;z-index: 10;}
    .feed .detail .feed_img .feed_img_item .total_img{display: block;position: absolute;top: 0;right: 0;border-bottom-left-radius: 3px;width: 35px;height: 21px;font-size: 12px;text-align: right;color: #fff;background-color: rgba(0,0,0,.62);background-image: url('data:image/png;base64,');background-repeat: no-repeat;background-position-x:5px;background-position-y:5px;background-size: 12px 10px;}
    .feed .detail .feed_img .feed_img_item .total_img .total_img_txt{display: block;margin: 5px;padding-left: 14px;}
    .feed .detail .feed_groupbody{position: relative;margin-top: 10px;margin-bottom: -1px;line-height: 18px;clear: both;}
    .feed .detail .feed_groupbody .feed_time{height: 18px;float: left;font-size: 11px;color: #777;}
    .feed .detail .feed_groupbody .icon_wrap{float: right;position: relative;top: -1px;margin-left: 25px;}
    .feed .detail .feed_groupbody .icon_wrap .groupreply {font-size: 11px;color: #acacac;}
    .feed .detail .feed_groupbody .icon_wrap .readnum {font-size: 11px;color: #acacac;}
    .feed .detail .feed_groupbody .icon_wrap .reply-icon {top: 1px;left: -16px;background-image: url('data:image/png;base64,');background-repeat: no-repeat;display: inline-block;position: absolute;width: 15px;height: 15px;background-size: 100%;}
    .feed .detail .feed_groupbody .icon_wrap .read-icon{top: 1px;left: -17px;background-image: url('data:image/png;base64,');display: inline-block;position: absolute;width: 15px;height: 15px;background-size: 100%;}
    .publish-btn{position: fixed;bottom: 20px;right: 15px;z-index: 100;width: 72px;height: 72px;background: url(http://pub.idqqimg.com/qqun/xiaoqu/mobile/img/../img/publish-detail-icon.png?6427bee5);background-size: 100% 100%;}
    .publish{position: fixed;left: 0;top: 0;bottom: 0;right: 0;background-color: #f0eff5;width: 100%;-webkit-user-select:none;-webkit-highlight:none;-webkit-tap-highlight-color:transparent;font-size: 0;}
    .publish .pub-con{background: #f0eff5;position: relative;top: 0;width: 100%;-webkit-user-select:none;-webkit-tap-highlight-color:transparent;}
    .publish .pub-con .pub-wrap{background: transparent;overflow: hidden;position: relative;}
