    left: 4px;
    z-index: 2;
    width: 14px;
    height: 1px;
    background: #fff;
    content: "";}

    #publish-form{display: none;}
    .loading{width: 100%;height: 30px; text-align:center;line-height: 30px;color: #777;}
    .up-entry-one{
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        opacity: 0;
        padding: 0;
        margin: 0;
        display: inline-block;
