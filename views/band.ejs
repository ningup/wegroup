  <body>
    <div id="monitor">Y position: <strong id="position">0</strong></div>
    <div id="wrapper">
      <div class="container">
          <div id="band_top_info" class="band_top_info">
            <% if(groupHeadImg=='default'){ %>
                <div class="top_portrait" style="background-image:url(http://m.tiebaimg.com/timg?mowebapp&quality=70&size=b150_150&cut_x=0&cut_w=0&cut_y=0&cut_h=0&sec=1369815402&di=bde46b9852e585f645d8f344fbf712e6&wh_rate=null&src=http%3A%2F%2Fimgsrc.baidu.com%2Fforum%2Fpic%2Fitem%2F34a85edf8db1cb131afb4e59db54564e93584bed.jpg);">
                <input class="up-entry-one" id="up-entry-one" type="file" accept="image/*" >
                </div>
            <% }else{ %>
                <% var temp_groupHeadImg = "background-image:url(" + groupHeadImg + ");"; %>
                <div class="top_portrait" style="<%= temp_groupHeadImg%>">
                <input class="up-entry-one" id="up-entry-one" type="file" accept="image/*" >
                </div>
            <% } %>
              <p class="top_title">
                  <span class="top_title_txt"><%= groupNickname%></span>
              </p>
              <p class="top_paragraph">
                  <span class="top_paragraph_label">话题</span>
                  <span class="top_paragraph_txt"><%= feedCnt%></span>
                  <span class="top_paragraph_label">关注</span>
                  <span class="top_paragraph_txt"><%= followersNum%></span>
              </p>
              <div class="top_sign_like">
                    <div class="sign_like_time">你已签到<span class="day_num" style="color:#f25824;"><%= cnt%></span>天</div>
                    <% if(isSignIn==0){ %>
                        <button class="sign_like_button no">签到</button>
                    <% }else{ %>
                        <button class="sign_like_button yes">已签到</button>
                    <% } %>
              </div>
          </div>
          <div class="feed_list">
              <ul class="feed_ul_list">
                <% for(var i=0; i<feeds.length; i++) {%>
                  <li class="feed" data-feedid="<%= feeds[i].getObjectId()%>">
                      <div class="detail">
                          <div class="feed_content">
                              <p class="feed_title"><%= feeds[i].get('feedTitle')%></p>
                              <p class="feed_txt"><%= feeds[i].get('feedContent')%></p>
                          </div>
                          <div class="feed_img">
                            <% if(feeds[i].get('feedImgArray').length>3){
                                    for(var q=0;q<3;q++){
                                        if(q==2){ %>
                                            <div class="feed_img_item" >
                                                  <img src="<%= feeds[i].get('feedImgArray')[q]%>" data-index="<%= q%>" data-src="">
                                                  <span class="total_img">
                                                      <span class="total_img_txt"><%= feeds[i].get('feedImgArray').length%></span>
                                                  </span>
                                              </div>
                                        <% }else{ %>
                                            <div class="feed_img_item" >
                                              <img src="<%=feeds[i].get('feedImgArray')[q]%>" data-index="<%= q%>" data-src="">
                                          </div>
                                        <% } %>
                                    <% } %>
                               <% }else{ %>
                               <% for(var j=0;j<feeds[i].get('feedImgArray').length;j++){ %>
                                  <div class="feed_img_item" >
                                      <img src="<%=feeds[i].get('feedImgArray')[j]%>" data-index="<%= j%>" data-src="">
                                  </div>
                                <% } %>
                            <% } %>
                          </div>
                          <div class="feed_groupbody">
                                <%var date = new Date();
                                var year = date.getFullYear() - feeds[i].getCreatedAt().getFullYear();
                                var month = date.getMonth() - feeds[i].getCreatedAt().getMonth();
                                var day = date.getDate() - feeds[i].getCreatedAt().getDate();
                                var hours = date.getHours() - feeds[i].getCreatedAt().getHours();
                                var minute = date.getMinutes() - feeds[i].getCreatedAt().getMinutes();
                                var last_time = "";
                                if(year>0){
                                    last_time= year + "年前";
                                }else if(month>0){
                                    last_time= month + "月前";
                                }else if(day>0){
                                    last_time= day + "天前";
                                }else if(hours>0){
                                    last_time= hours + "小时前";
                                }else if(minute>0){
                                    last_time= minute + "分钟前";
                                }else{
                                    last_time= "刚刚";
                                }%>
                              <div class="feed_time"><%= last_time%></div>
                              <div class="icon_wrap">
                                  <i class="reply-icon"></i>
                                  <span class="groupreply"><%= feeds[i].get('commentCnt')%></span>
                              </div>
                              <div class="icon_wrap">
                                  <i class="read-icon"></i>
                                  <span class="readnum"><%= feeds[i].get('visitCnt')%></span>
                              </div>
                          </div>
                      </div>
                  </li>
                <% } %>
                 
              </ul>
              <div class="loading"></div>
          </div>
      </div>
    </div>
    <div class="publish-btn" data-toggle="modal" data-target="#myModal"></div>
    <div class="modal fade publish" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="false">
        <div class="pub-con">
            <div class="pub-wrap">
                <div class="pub-theme">
                    <input maxlength="200" spellcheck="false" class="ipt-theme" type="text" placeholder="标题，4-25个字">
                </div>
                <div class="pub-editor">
                    <textarea spellcheck="false" class="editor" placeholder="内容，10-700个字"></textarea>
                </div>
                <ul class="pub-type">
                    <!--<li id="selectPic" class="pic-type active" title="添加图片"></li>
                    <li id="selectFace" class="pub-face" title="添加表情"></li>
                    <li class="pub-flex"></li>-->
                    <li class="pub-btn "> <button class="pub-publish">发表</button> <button class="pub-cancel">关闭</button> </li>
                </ul>
                <ul class="pub-pics"> 
                    <li class="up-entry"> <input class="upfile up-entry-two" id="inputField" type="file" accept="image/*" multiple=""> </li> 
                </ul>
            </div>
        </div>
        <form id="publish-form" method="post" action="/feed/post">
            <input type="hidden" name="feedType" id="form-feedType" value="">
            <input type="hidden" name="username" id="form-username" value="">
            <input type="hidden" name="feedTitle" id="form-feedTitle" value="">
            <textarea type="hidden" name="feedContent" id="form-feedContent"></textarea>
            <input type="hidden" name="imgurl" id="form-imgurl" value="">
        </form>
        <!--<form id="feed-post" method="get" action="https://open.weixin.qq.com/connect/oauth2/authorize?appid=wx88cb5d33bbbe9e75&redirect_uri=http://dev.wegroup.avosapps.com/feed/detail&response_type=code&scope=snsapi_base&state=123#wechat_redirect">
        	<input type="hidden" name="feedObjId" id="feedObjId" value="">
        	<input type="hidden" name="groupObjId" id="groupObjId" value="<%= groupObjId%>">
        </form>-->
        <a href="" style="display:none;" id="feed-post"><span style="display:none;" id="feed-click">跳转</span></a>
    </div>
    <div class="pswp" tabindex="-1" role="dialog" aria-hidden="true">

        <!-- Background of PhotoSwipe. 
             It's a separate element, as animating opacity is faster than rgba(). -->
        <div class="pswp__bg"></div>

        <!-- Slides wrapper with overflow:hidden. -->
        <div class="pswp__scroll-wrap">

            <!-- Container that holds slides. PhotoSwipe keeps only 3 slides in DOM to save memory. -->
            <div class="pswp__container">
                <!-- don't modify these 3 pswp__item elements, data is added later on -->
                <div class="pswp__item"></div>
                <div class="pswp__item"></div>
                <div class="pswp__item"></div>
            </div>

            <!-- Default (PhotoSwipeUI_Default) interface on top of sliding area. Can be changed. -->
            <div class="pswp__ui pswp__ui--hidden">

                <div class="pswp__top-bar">

                    <!--  Controls are self-explanatory. Order can be changed. -->

                    <div class="pswp__counter"></div>

                    <button class="pswp__button pswp__button--close" title="Close (Esc)"></button>

                    <!-- Preloader demo http://codepen.io/dimsemenov/pen/yyBWoR -->
                    <!-- element will get class pswp__preloader--active when preloader is running -->
                    <div class="pswp__preloader">
                        <div class="pswp__preloader__icn">
                          <div class="pswp__preloader__cut">
                            <div class="pswp__preloader__donut"></div>
                          </div>
                        </div>
                    </div>
                </div>

                <div class="pswp__share-modal pswp__share-modal--hidden pswp__single-tap">
                    <div class="pswp__share-tooltip"></div> 
