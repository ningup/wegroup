    <script type="text/javascript">
    var reply_flag = "<%= commentObjId%>";
    var toWhom = "<%= toWhom%>";
    var replyCommentId = "<%= commentObjId%>";
    var inWhichComment = "<%= commentObjId%>";
    var chufa_flag = "0";
    var reply_num = 0;
    $(document).ready(function(){
        //$("ul>li:last").css("margin-bottom","45px");
        $(".delete_feed").click(function(){
            $(".delete_feed_form").submit();
        });
        $(document).on('click',".del",function(){
            var commentObjId = $(this).closest("li.item").attr("data-reply");
            var aj = $.ajax({
                    url:'/comment/remove',
                    data:{
                        "commentObjId":commentObjId
                    },
                    type:'post',
                    dataType:'json',
                    success:function(data){
                        //alert("coming");
                        if(data.isComment=="0"){
                            $("li.item").each(function(){
                                if($(this).attr("data-replyid")==data.cid){
                                    $(this).remove();
                                }
                            });
                        }
                    }
            });
        });
        $(document).on('click',"li.item",function(){
          //alert("heh");
          reply_flag = $(this).attr("data-reply");
          toWhom = $(this).attr("data-user");
          //alert(reply_flag);
          //alert(toWhom);
          var li_nickname = $(this).find(".user_name").html();
          li_nickname = "回复 " + li_nickname + ":";
          $(".editor").val("");
          $(".editor").attr("placeholder",li_nickname);
          $(".editor").focus();
        });
        $(".editor").blur(function(){
          setTimeout(function(){
            //alert("hei");
            if (chufa_flag=="1") {
              chufa_flag="0";
              return;
            }
            if (reply_flag != "<%= commentObjId%>") {$(".editor").val("");}
            $(".editor").attr("placeholder","我也来说一句....");
            reply_flag = "<%= commentObjId%>";
            toWhom = "<%= toWhom%>";
          },250);
        });
        $(".pub-publish").click(function(){
          chufa_flag = "1";
          var content = $(".editor").val();
          $(".editor").val("");
          var aj = $.ajax({
                url:'/comment',
                data:{
                    "username":"<%= username%>",
                    "groupObjId":"<%= groupObjId%>",
                    "content":content,
                    "toWhom":toWhom,
                    "feedObjId":"<%= feedObjId%>",
                    "commentType":"text",
                    "isReply":"1",
                    "replyCommentId":reply_flag,
                    "inWhichComment":"<%= commentObjId%>"
                },
                type:'post',
                dataType:'json',
                success:function(data){
                    console.log(data);
                    //$("ul>li:last").css("margin-bottom","");
                    if(isHave=="0"){
                          if(data.replyCommentId=="<%= commentObjId%>"){
                              var date = new Date();
                              var date1 = new Date(data.replyTime);
                              var year = date.getFullYear() - date1.getFullYear();
                              var month = date.getMonth() - date1.getMonth();
                              var day = date.getDate() - date1.getDate();
                              var hours = date.getHours() - date1.getHours();
                              var minute = date.getMinutes() - date1.getMinutes();
                              var last_time = "";
                              if(year>0){
                                last_time= year + "年前";
                              }else if(month>0){
                                last_time= month + "月前";
                              }else if(day>0){
                                last_time= day + "天前";
                              }else if(hours>0){
                                last_time= hours + "小时前";
                              }else if(minute>0){
                                last_time= minute + "分钟前";
                              }else{
                                last_time= "刚刚";
                              }
                              var str = "<li class=\"item\" id=\"temp\" data-reply=\""+data.commentObjId+"\" data-user=\""+ data.username +"\"><div class=\"fmain j_floor_main\"><div class=\"floor_footer_item\"><a href=\"\" class=\"user_name \">"+data.nickname+"</a>:<span class=\"floor_content\">"+data.content+"</span><span class=\"list_item_time\">"+/*data.replyTime*/last_time+"</span><span class=\"del\">删除</span></div></div></li>";
                              $("ul.reply-list").append(str);
                          }else{
                              var date = new Date();
                              var date1 = new Date(data.replyTime);
                              var year = date.getFullYear() - date1.getFullYear();
                              var month = date.getMonth() - date1.getMonth();
                              var day = date.getDate() - date1.getDate();
                              var hours = date.getHours() - date1.getHours();
                              var minute = date.getMinutes() - date1.getMinutes();
                              var last_time = "";
                              if(year>0){
                                last_time= year + "年前";
                              }else if(month>0){
                                last_time= month + "月前";
                              }else if(day>0){
                                last_time= day + "天前";
                              }else if(hours>0){
                                last_time= hours + "小时前";
                              }else if(minute>0){
                                last_time= minute + "分钟前";
                              }else{
                                last_time= "刚刚";
                              }
                              var str = "<li class=\"item\" id=\"temp\" data-reply=\""+data.commentObjId+"\" data-user=\""+ data.username +"\"><div class=\"fmain j_floor_main\"><div class=\"floor_footer_item\"><a href=\"\" class=\"user_name \">"+data.nickname+"</a>:<span class=\"floor_content\">回复&nbsp;<a href=\"\">"+data.toNickname+"</a>&nbsp;:"+data.content+"</span><span class=\"list_item_time\">"+/*data.replyTime*/last_time+"</span><span class=\"del\">删除</span></div></div></li>";
                              $("ul.reply-list").append(str);
                          } 
                    }
                    //$("ul>li:last").css("margin-bottom","45px");
                    reply_flag = "<%= commentObjId%>";
                    toWhom = "<%= toWhom%>";
                    $(".editor").attr("placeholder","我也来说一句....");
                    myScroll.refresh();
                },
                error:function(){
                    reply_flag = "<%= commentObjId%>";
                    toWhom = "<%= toWhom%>";
                    $(".editor").attr("placeholder","我也来说一句....");
                    myScroll.refresh();
                }
            });
        });
        $(".ping-pic>img").click(function(){
          event.stopPropagation();
            var pswpElement = $(".pswp")[0];
            var img_index = $(this).attr("data-index");
            var items = [];
            var i=0;
            //console.log(img_index);
            $(".ping-pic>img").each(function(){
                var image = new Image();
                var width;
                var height;
                image.src = $(this).attr("src");
                width = image.naturalWidth;
                height = image.naturalHeight;
                items[i] = {
                    src:$(this).attr("src"),
                    w:width,
                    h:height
                };
                i++;
            });
            img_index = parseInt(img_index);
            var options = {
                // optionName: 'option value'
                // for example:
                index: img_index, // start at first slide
                history: false,
                focus: false,
                tapToClose: true,
                tapToToggleControls: false,
                showAnimationDuration: 0,
                hideAnimationDuration: 0
            };
            var gallery = new PhotoSwipe( pswpElement, PhotoSwipeUI_Default, items, options);
            gallery.init();
        });
    });
    </script>
