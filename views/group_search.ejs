<!doctype html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<title>查找微群</title>
<meta name="renderer" content="webkit">
<meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
<meta name="Keywords" content="微群助手－查找群" />
<meta name="Description" content="微信公众平台上的公众号－－微群助手" />
<link rel="stylesheet" type="text/css" href="/stylesheets/index.css">
<link rel="stylesheet" type="text/css" href="/stylesheets/skin.css">
<link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.css">
<script language="javascript" type="text/javascript" src="/javascript/jquery.min.js"></script>
<script language="javascript" type="text/javascript" src="/javascript/bootstrap.js"></script>
</head>
<body>
	<div class="search_button">
		<div class="input-group form-control">
			<span class="glyphicon glyphicon-search" id="sizing-addon2"></span>
			<input type="text" id="search_box" class="" placeholder="查找微群" aria-describedby="sizing-addon2">
			<span class="input-group-addon" id="search_cancel" style="background: none;border: none;">取消</span>
		</div>
	</div>
	<div class="chat_list">
		<ul class="list-unstyled">
        <!--<for start="1" end="5">-->
        <% for(var i=0; i<groups.length; i++) {%>
			<div class="media media_content" data-toggle="modal" data-target="#channelModal" data-whatever="@mdo" style="margin-top:0" data-skin="skin4"><a href="#" class="symbol_img" data-skin_factor="before_bg">
				<div class="media-left media-middle"><img data-skin_factor="bg" data-skin_factor="before_bg" class="media-object img-circle img-thumbnail" data-src="holder.js/46x46" alt="46x46" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 64px; height: 64px;"></div>
				<div class="media-body media-middle">
					<h5 class="media-heading" style="margin-top: 0;"><strong><!--第 {$i} 个群--><%= groups[i].get('nickname')%></strong></h5><p style="color:#999;margin-bottom: 0;"><small class="user_count" data-icon="chat-member"> {$i*56}个成员</small>  <small class="user_leader"><!--团长：某某某--><%= groups[i].get('nicknameOfCuser')%></small></p><h6 style="margin:8px 0;">{$i} : 简介。</h6>  
				</div>              
			</a></div>
		<% } %>
		<!--</for>-->
        </ul>
        
		<div id="channelModal" class="modal fade bs-album-modal-sm text-center" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" data-skin="skin4">
			<div class="modal-dialog modal-sm">
				<div class="modal-content">
                	<div data-skin_factor="border" data-skin_factor="before_bg" class="media-object" data-holder-rendered="true" style="width: 100%;height: 220px;border-bottom: solid 15px;background-size: 100% 100%;background: url(http://www.w3school.com.cn/i/bg_flower.gif) no-repeat center;padding-top: 150px;color:#FFF"><div class="bl-wi-color" style="height: 60px;top: 145px;"><h3></h3></div></div>
					<div class="modal-body">
                        <small></small>
                        <p class="text-center"><for start="1" end="6"><img class="img-circle" data-src="holder.js/46x46" alt="46x46" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 40px; height: 40px; margin:5px 3px"></for></p>
						<h5></h5>
						<button type="button" class="btn btn-success" style="width: 100%;">加入该微群</button>
					</div>      
				</div>
			</div>
		</div>       
        
	</div>
<script language="javascript" type="text/javascript">
$("#search_cancel").hide();
$(function(){
$("#search_box").focus(function(){$("#search_cancel").show();})
$("#search_box").blur(function(){$("#search_cancel").hide();})
$("#search_cancel").click(function(){window.open("./list","_self");})
/*$(".channel").click(function(){
	$('#channelModal').modal('show');
})*/
$('#channelModal').on('show.bs.modal', function (event) {
	//console.debug(this);
  var channel = $(event.relatedTarget);
  var modal = $(this)
  var recipient = channel.data('skin');//.text('New message to ' + recipient)
  channel_img = channel.find('img').text();
  channel_name = channel.find('h5').text();
  channel_contant = channel.find('h6').text();
  channel_small = channel.find('small').text();
  
  modal.find('img').text(channel_img)
  modal.find('h3').text(channel_name)
  modal.find('h5').text(channel_contant)
  modal.find('small').text(channel_small)
  modal.find('.modal-body input').val(recipient)
})
})
</script>
</body>
</html>